{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "construction/staticfiles_build"  
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/construction/staticfiles_build/$1"  
    },
    {
      "src": "/(.*)",
      "dest": "ps/wsgi.py"
    }
  ]
}
